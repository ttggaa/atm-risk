    <insert id="genBasicCheckItem" >
        insert into `sta_basic_check_items` (`nid`, `phone`, `idcard_check`, `idcard_check_comment`, `email_check`, `email_check_comment`, `address_check`, `address_check_comment`, `call_data_check`, `call_data_check_comment`, `idcard_match`, `idcard_match_comment`, `name_match`, `name_match_comment`, `is_name_and_idcard_in_court_black`, `is_name_and_idcard_in_court_black_comment`, `is_name_and_idcard_in_finance_black`, `is_name_and_idcard_in_finance_black_comment`, `is_name_and_mobile_in_finance_black`, `is_name_and_mobile_in_finance_black_comment`, `mobile_silence_3m`, `mobile_silence_3m_comment`, `mobile_silence_6m`, `mobile_silence_6m_comment`, `arrearage_risk_3m`, `arrearage_risk_3m_comment`, `arrearage_risk_6m`, `arrearage_risk_6m_comment`, `binding_risk`, `binding_risk_comment`)
        values (
        #{nid}, #{ phone}, #{ idcard_check}, #{ idcard_check_comment}, #{ email_check}, #{ email_check_comment}, #{ address_check}, #{ address_check_comment}, #{ call_data_check}, #{ call_data_check_comment}, #{ idcard_match}, #{ idcard_match_comment}, #{ name_match}, #{ name_match_comment}, #{ is_name_and_idcard_in_court_black}, #{ is_name_and_idcard_in_court_black_comment}, #{ is_name_and_idcard_in_finance_black}, #{ is_name_and_idcard_in_finance_black_comment}, #{ is_name_and_mobile_in_finance_black}, #{ is_name_and_mobile_in_finance_black_comment}, #{ mobile_silence_3m}, #{ mobile_silence_3m_comment}, #{ mobile_silence_6m}, #{ mobile_silence_6m_comment}, #{ arrearage_risk_3m}, #{ arrearage_risk_3m_comment}, #{ arrearage_risk_6m}, #{ arrearage_risk_6m_comment}, #{ binding_risk}, #{ binding_risk_comment}
        )
    </insert>

    <insert id="genCheckBlackInfo" parameterType="com.alibaba.fastjson.JSONObject" >
        insert into `sta_check_black_info` (`nid`, `phone`, `contacts_class1_cnt`, `contacts_class1_blacklist_cnt`, `contacts_router_cnt`, `contacts_class2_blacklist_cnt`, `contacts_router_ratio`, `phone_gray_score`)
        values(#{nid}, #{phone}, #{contacts_class1_cnt}, #{contacts_class1_blacklist_cnt}, #{contacts_router_cnt}, #{contacts_class2_blacklist_cnt}, #{contacts_router_ratio}, #{phone_gray_score}
        )
    </insert>

    <insert id="genCallFamilyDetail" parameterType="com.alibaba.fastjson.JSONObject" >
        insert into `sta_call_family_detail` (`nid`, `phone`, `app_point`, `app_point_zh`, `item_1m`, `item_3m`, `item_6m`, `avg_item_3m`, `avg_item_6m`)
        values(#{nid}, #{phone}, #{app_point}, #{app_point_zh}, #{item_1m}, #{item_3m}, #{item_6m}, #{avg_item_3m}, #{avg_item_6m}
        )
    </insert>

    <insert id="genCallMidnight" parameterType="com.alibaba.fastjson.JSONObject" >
        insert into `sta_call_duration_detail` (`nid`, `phone`, `time_step`, `time_step_zh`, `dialed_time`, `dialed_cnt`, `total_time`, `total_cnt`, `dial_time`, `dial_cnt`, `latest_call_time`, `farthest_call_time`, `uniq_num_cnt`, `add_time`, `update_time`)
        values(
        #{nid}, #{phone}, #{time_step}, #{time_step_zh}, #{dialed_time}, #{dialed_cnt}, #{total_time}, #{total_cnt}, #{dial_time}, #{dial_cnt}, #{latest_call_time}, #{farthest_call_time}, #{uniq_num_cnt}, #{add_time}, #{update_time}
        )
    </insert>

    <insert id="genCallSilentAreasBill" parameterType="com.alibaba.fastjson.JSONObject" >
        insert into `sta_silent_areas_bill` (`nid`, `phone`, `region_cnt`, `trip_cnt`, `avg_charge`, `phone_silent`)
        values(
        #{nid}, #{phone}, #{region_cnt}, #{trip_cnt}, #{avg_charge}, #{phone_silent}
        )
    </insert>

    <select id="getAllReqNid" parameterType="String" resultType="String">
        SELECT DISTINCT r.nid FROM risk_decision_req_log r
    </select>

    <select id="getSmBorrow" parameterType="String" resultType="String">
        SELECT DISTINCT r.order_no AS nid FROM zx_atm.zx_repay_order r
        WHERE NOT EXISTS (SELECT 1 FROM zx_risk.`sta_sm_borrows` WHERE r.order_no = nid)
    </select>

    <select id="getRiskAnaLysisNid" parameterType="String" resultType="String">
        SELECT DISTINCT r.order_no AS nid FROM zx_atm.zx_repay_order r
        WHERE NOT EXISTS (SELECT 1 FROM zx_risk.`sta_call_risk_analysis` WHERE r.order_no = nid)
    </select>

    <select id="getBasicCheckItem" parameterType="String" resultType="String">
        SELECT DISTINCT r.order_no AS nid FROM zx_atm.zx_repay_order r
        WHERE NOT EXISTS (SELECT 1 FROM zx_risk.`sta_basic_check_items` WHERE r.order_no = nid)
    </select>

    <select id="getCheckBlackInfo" parameterType="String" resultType="String">
        SELECT DISTINCT r.order_no AS nid FROM zx_atm.zx_repay_order r
        WHERE NOT EXISTS (SELECT 1 FROM zx_risk.`sta_check_black_info` WHERE r.order_no = nid)
    </select>

    <select id="getCallFamilyDetail" parameterType="String" resultType="String">
        SELECT DISTINCT r.order_no AS nid FROM zx_atm.zx_repay_order r
        WHERE NOT EXISTS (SELECT 1 FROM zx_risk.`sta_call_family_detail` WHERE r.order_no = nid)
    </select>

</mapper>